<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cosc592.meetingscheduler">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MeetingActivity"
            android:label="@string/title_activity_meeting"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MemberActivity"
            android:label="@string/title_activity_member"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CommitteeActivity"
            android:label="@string/title_activity_committee"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LoginKeyActivity"
            android:label="@string/title_activity_login_key"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AddCommitteeActivity" />
        <activity android:name=".AddMeetingActivity" />
        <activity android:name=".AddMemberActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>